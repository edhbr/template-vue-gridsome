{
  "name": "template-vue-gridsome",
  "version": "0.2.0",
  "description": "A Gridsome starter pack to build app with Vue.js",
  "author": "lihbr",
  "license": "SEE LICENSE IN LICENSE",
  "scripts": {
    "dev": "gridsome develop",
    "build": "gridsome build",
