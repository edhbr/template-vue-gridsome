  "dependencies": {
    "@gridsome/plugin-sitemap": "0.2.2",
    "@gridsome/source-filesystem": "0.6.1",
    "@gridsome/transformer-json": "0.2.1",
    "color": "3.1.2",
    "dotenv": "8.1.0",
    "gridsome": "0.7.7",
    "gridsome-plugin-gtm": "0.1.1",
    "gridsome-plugin-netlify-cms": "1.0.9",
    "is-mobile": "2.1.0",
    "marked": "0.7.0",
    "mouse-wheel": "1.2.0",
    "netlify-cms": "2.9.7",
    "object-fit-images": "3.2.4",
    "prismjs": "1.17.1",
    "stats-js": "1.0.1",
    "tailwindcss": "1.1.2",
    "tailwindcss-transitions": "2.1.0",
    "vue-functional-data-merge": "3.1.0",
    "vuex": "3.1.1"
